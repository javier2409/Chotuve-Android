{
  "expo": {
    "name": "Chotuve",
    "slug": "Chotuve",
    "platforms": [
      "android"
    ],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#11151a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.fiuba.chotuveapp",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "http",
              "host": "*.chotuve.video"
            },
            {
              "scheme": "https",
              "host": "*.chotuve.video"
            },
            {
              "scheme": "http",
              "host": "chotuve.video"
            },
            {
              "scheme": "https",
              "host": "chotuve.video"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "notification": {
      "icon": "./assets/notification.png",
      "androidMode": "collapse",
      "androidCollapsedTitle": "Notificaciones"
    },
    "scheme":"chotuve"
  }
}
